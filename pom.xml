<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project xmlns="http://maven.apache.org/SETTINGS/1.1.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd">

	<modelVersion>4.0.0</modelVersion>

	<groupId>bobkubista.examples</groupId>
	<artifactId>main</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<packaging>pom</packaging>

	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
	</properties>

	<modules>
		<module>build</module>
		<module>dependencies</module>
		<module>webapps</module>
		<module>utils</module>
		<module>services</module>
	</modules>

	<distributionManagement>
	</distributionManagement>

	<scm>
		<url>https://github.com/bobkubista/examples.git</url>
	</scm>

	<profiles>
		<profile>
			<id>sonar</id>
			<properties>
				<sonar.jdbc.url></sonar.jdbc.url>
				<sonar.jdbc.driverClassName>com.mysql.jdbc.Driver</sonar.jdbc.driverClassName>
				<sonar.jdbc.username></sonar.jdbc.username>
				<sonar.jdbc.password></sonar.jdbc.password>
				<sonar.host.url></sonar.host.url>
			</properties>
		</profile>
	</profiles>
</project>
